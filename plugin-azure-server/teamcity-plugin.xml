<?xml version="1.0" encoding="UTF-8"?>


<teamcity-plugin xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:shemas-jetbrains-com:teamcity-plugin-v1-xml">
  <info>
    <name>cloud-azure-arm</name>
    <display-name>Azure Resource Manager Cloud Support</display-name>
    <version>@Version@</version>
    <description>Allows using virtual machines in Microsoft Azure cloud to scale the pool of TeamCity build agents.</description>
    <download-url>https://github.com/JetBrains/teamcity-azure-plugin</download-url>
    <vendor>
      <name>JetBrains</name>
      <url>https://www.jetbrains.com/</url>
    </vendor>
  </info>
  <requirements min-build="40000" />
  <deployment use-separate-classloader="true"  node-responsibilities-aware="true"/>
</teamcity-plugin>
